
function validacao()
{
	if(document.form.nome.value==""){
		alert("Por favor, preencha o campo Nome");
		document.form.nome.focus();
		return false;
	}

	if(document.form.nome.value.lenght < 2){
		alert("O nome deve mais que dois caractÃ©res");
		document.form.nome.focus();
		return false;
	}
	
	
}